SELECT tr.tax_class_id, tr.rate AS rate, tr.rate_prefix AS rate_prefix, tr.threshold_condition AS threshold_condition, tr.threshold AS threshold, tr.tax_exempt_groups AS tax_exempt_groups, COALESCE( td1.title,td2.title) as title, COALESCE( NULLIF(trd1.description, ''), NULLIF(td1.description, ''), NULLIF(trd2.description, ''), NULLIF(td2.description, ''), COALESCE( td1.title,td2.title) ) as description, tr.priority FROM ac_tax_rates tr LEFT JOIN ac_tax_rate_descriptions trd1 ON (tr.tax_rate_id = trd1.tax_rate_id AND trd1.language_id = '1') LEFT JOIN ac_tax_rate_descriptions trd2 ON (tr.tax_rate_id = trd2.tax_rate_id AND trd2.language_id = '1') LEFT JOIN ac_tax_classes tc ON tc.tax_class_id = tr.tax_class_id LEFT JOIN ac_tax_class_descriptions td1 ON (tc.tax_class_id = td1.tax_class_id AND td1.language_id = '1') LEFT JOIN ac_tax_class_descriptions td2 ON (tc.tax_class_id = td2.tax_class_id AND td2.language_id = '1') WHERE (tr.zone_id = '0' OR tr.zone_id = '3613') AND tr.location_id in (SELECT z2l.location_id FROM ac_zones_to_locations z2l, ac_locations l WHERE z2l.location_id = l.location_id and z2l.zone_id = '3613') ORDER BY tr.priority ASC
