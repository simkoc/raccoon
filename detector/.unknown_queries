select `oxid` from oxuser where oxuser.oxactive = 1 and oxuser.oxpassword = BINARY MD5( CONCAT( 'wrongpassword', UNHEX( oxuser.oxpasssalt ) ) ) and oxuser.oxusername = 'user@example.com'
